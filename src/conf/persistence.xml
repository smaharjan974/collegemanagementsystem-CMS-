<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="InfoVistaCMSPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>com.spring.entity.Event</class>
    <class>com.spring.entity.Batchsemester</class>
    <class>com.spring.entity.Semesterrname</class>
    <class>com.spring.entity.Attendance</class>
    <class>com.spring.entity.Faculty</class>
    <class>com.spring.entity.Admindb</class>
    <class>com.spring.entity.Marks</class>
    <class>com.spring.entity.Batch</class>
    <class>com.spring.entity.Semester</class>
    <class>com.spring.entity.Teacherdb</class>
    <class>com.spring.entity.Studentdb</class>
    <class>com.spring.entity.Guardiandb</class>
    <class>com.spring.entity.Subject</class>
    <class>com.spring.entity.Batchsemster</class>
    <class>com.spring.entity.Semestername</class>
    <class>com.spring.entity.Teachersubject</class>
    <class>com.spring.entity.Assignment</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/majorproject?zeroDateTimeBehavior=convertToNull"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value=""/>
    </properties>
  </persistence-unit>
</persistence>
